{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "traceviz-angular": {
      "projectType": "library",
      "root": "projects/traceviz-angular",
      "sourceRoot": "projects/traceviz-angular/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/traceviz-angular/tsconfig.lib.json",
            "project": "projects/traceviz-angular/ng-package.json"
          }
        }
      }
    },
    "testhost": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/testhost",
      "sourceRoot": "projects/testhost/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/testhost/src/test.ts",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/testhost/tsconfig.spec.json",
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
